    "eslint-plugin-react": "7.20.5",
    "eslint-plugin-react-app": "6.2.2",
    "husky": "4.2.5",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "react-scripts": "3.4.1",
    "spectron": "12.0.0",
    "standard-version": "8.0.2",
    "typescript": "3.9.7",
    "wait-on": "5.2.0"
  },
  "scripts": {
    "prebuild": "yarn prepare",
    "build": "concurrently \"env-cmd -f ./.env.local react-scripts build\"",
    "dist": "env-cmd -f ./.env.local electron-builder",
    "dist:win": "env-cmd -f ./.env.local electron-builder -w --x64 --ia32",
    "dist:mac": "env-cmd -f ./.env electron-builder -m",
    "dist:linux": "env-cmd -f ./.env electron-builder -l",
    "dist:posix": "env-cmd -f ./.env electron-builder -ml",
    "dist:win:docker": "./scripts/buildWindowsWithDocker.sh",
    "dist:all": "run-s dist:posix dist:win",
    "lint": "eslint .",
    "prestart": "yarn prepare && env-cmd -f ./.env.local node scripts/setup.js",
    "start": "concurrently \"env-cmd -f ./.env.local react-scripts start\" \"wait-on http://localhost:3000 && env-cmd -f ./.env.local electron .\"",
    "premocha": "yarn prepare",
    "mocha": "concurrently \"env-cmd -f ./.env.test react-scripts start\" \"wait-on http://localhost:3000 && mocha --require @babel/register \"test/**/*.test.js\"\"",
    "prettier:check": "prettier --check {src,public,test}/**/*.js",
    "prettier:write": "prettier --write {src,public,test}/**/*.js",
    "hooks:uninstall": "node node_modules/husky/husky.js uninstall",
    "hooks:install": "node node_modules/husky/husky.js install",
    "ci": "react-scripts test --env=jsdom --coverage",
    "prepare": "mkdirp public/src/shared && cpy \"src/shared/**/*.js\" \"public/shared\"",
    "setup": "node scripts/setup.js",
    "test:once": "cross-env CI=true env-cmd -f ./.env.test react-scripts test --env=jsdom",
    "test": "react-scripts test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
